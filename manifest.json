{
  "manifest_version": 2,

  "name": "Paint!",
  "description": "This extension allows you to paint in a little window :)",
  "version": "1.0",
  "icons":{
    "48":"icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Paint Here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
